{
  "name": "tele2contests-landingpage",
  "main": "index.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && yarn run -s serve || PORT=3000 yarn run -s dev",
    "build": "preact build --template ./src/template.html",
    "serve": "preact build --template ./src/template.html && preact serve",
    "dev": "preact watch --template ./src/template.html",
    "test": "eslint src && preact test"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "eslint": "^4.5.0",
    "eslint-config-synacor": "^1.1.0",
    "if-env": "^1.0.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.1.0",
    "postcss-import": "^10.0.0",
    "postcss-mixins": "^6.1.0",
    "postcss-nested": "^2.1.2",
    "preact-cli": "^1.4.1",
    "preact-cli-sw-precache": "^1.0.3"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "cookie": "^0.3.1",
    "node-sass": "^4.5.3",
    "normalize-wheel": "^1.0.1",
    "preact": "^8.2.4",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.5.7",
    "react-input-mask": "^1.0.7",
    "sass-loader": "^6.0.6"
  }
}
